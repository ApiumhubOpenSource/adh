{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": [
            "es2015",
            "dom"
        ],
        "declaration": true,
        "declarationMap": true,
        "outDir": "./dist",
        "removeComments": true,
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}